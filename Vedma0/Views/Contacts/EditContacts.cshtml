@model Vedma0.Models.ViewModels.ContactList
@{

    ViewData["Title"] = "EditContacts";
    Layout = "~/Views/Shared/_Console.cshtml";
}

<h2>EditContacts</h2>
<form asp-action="Edit">
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayName("Имя")
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in (IList<Vedma0.Models.ViewModels.Selectable>)ViewBag.Characters)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>

                    <td>
                        <input type="checkbox" name="@item.Id" value="false">
                    </td>
                </tr>
            }
        </tbody>
    </table>
    <div class="form-group">
        <input type="submit" value="Сохранить" class="btn btn-default" />
    </div>
</form>